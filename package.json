    "knex": "^0.17.6",
    "mysql": "^2.17.1",
    "objection": "^1.6.9",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@playlyfe/gql": "^2.6.1",
    "nodemon": "^1.19.1",
    "eslint": "^6.0.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.3"
  }
